{
  "name": "vue-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm-run-all creat local",
    "build": "cross-env NODE_ENV=production npm-run-all creat serve delete",
